/* pages/C_map/C_map.wxss */
page{height:100%}
map{
  width: 100%;
  height: 90%;

}
.map_wrap{
  width: 100%;
  height: 90%;
     position: relative;
  z-index: 1; 
}
navigator{
  position: absolute;
  z-index: 99999!important;
}
navigator image{
  display: block;
  width: 100%;
}



.take,
.phone,
.me,
.more,
contact-button{
  position: absolute;
  width: 80rpx;
  border-radius: 50%;
  z-index: 9;
}
/* 客服 */
.take,
contact-button{
  right: 20rpx;
  bottom: 50rpx;
}
contact-button{
  display: block;
  opacity: 0;
  z-index: 10;
}
/* 电话 */
.phone{
  right: 20rpx;
  bottom: 150rpx;
}




/* 预约顾问 */
.server{
  position: absolute;
  bottom: 50rpx;
  left: 50%;
  transform: translate(-50%,0);
  width: 200rpx;
  padding: 10rpx;
  text-align: center;
  background: #48cfad;
  color: #ffffff;
  border-radius: 3px;
}

.header{
  height: 100rpx;
  position: relative
}
.header .input{
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 450rpx;
}
.header .me,
.header .more{
  width: 60rpx;
  top: 50%;
  transform: translateY(-50%)
}
/* 我 */
.me{
  left: 20rpx;
}

/* 更多 */
.more{
  right: 20rpx;
}