/* pages/C_map/C_map.wxss */
page{height:100%}
map{
  width: 100%;
  height: 100%;
}

navigator image{
  display: block;
  width: 100%;
}

.take,
.phone,
.me,
.more,
contact-button{
  position: absolute;
    width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  z-index: 9;
}
/* 客服 */
.take,
contact-button{
  right: 20rpx;
  bottom: 50rpx;
}
contact-button{
  display: block;
  opacity: 0;
  z-index: 10;
}
/* 电话 */
.phone{
  right: 20rpx;
  bottom: 150rpx;
}

/* 我 */
.me{
  left: 20rpx;
  top: 20rpx;
}

/* 更多 */
.more{
  right: 20rpx;
  top: 20rpx;
}


/* 预约顾问 */
.server{
  position: absolute;
  bottom: 50rpx;
  left: 50%;
  transform: translate(-50%,0);
  width: 200rpx;
  padding: 10rpx;
  text-align: center;
  background: #48cfad;
  color: #ffffff;
  border-radius: 3px;
}