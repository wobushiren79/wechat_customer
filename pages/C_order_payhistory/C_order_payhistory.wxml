<view class="page">

  <view class="page__bd">
    <view class="weui-cells__title">管理费到期时间</view>
    <view class="weui-panel weui-panel_access">
      <!--<view class="weui-panel__hd"></view>-->
      <view class="weui-panel__bd">
        <navigator url="" class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">

          <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
            <view class="weui-media-box__title">
              <view class="">{{danqian.endDate}}</view>

            </view>
            <!--<view class="weui-media-box__desc">{{danqian.payDate}}</view>-->
          </view>

        </navigator>

      </view>
    </view>


    <view class="weui-cells__title">管理费缴纳记录</view>
    <view class="weui-panel weui-panel_access">
      <!--<view class="weui-panel__hd"></view>-->
      <view class="weui-panel__bd">
      <block wx:for="{{list}}">
        <navigator url="" class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">

          <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
            <view class="weui-media-box__title">
              <view class="">{{item.beginDate}}~{{item.endDate}}</view>
              <view class="">￥{{item.payAmount}}</view>
            </view>
            <view class="weui-media-box__desc">{{item.payDate}}</view>
          </view>
          <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
            <image class="weui-media-box__thumb" src="../../images/icon_pay_wx.png" />
          </view>
        </navigator>
</block>
      </view>
    </view>
  </view>
</view>